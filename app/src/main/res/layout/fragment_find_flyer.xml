        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_margin="16dp">
            <com.example.membersmapstatistics.VerticalViewPager
                android:id="@+id/send_viewPager"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"/>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="1">
                <ImageView
                    android:id="@+id/find_flyer_coupon_image"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:scaleType="centerInside"
                    android:src="@drawable/ic_store_black_48px"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Date"
                    android:gravity="center"
                    android:drawableLeft="@drawable/ic_event_black_24px"
                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                    android:layout_margin="8dp"/>
                <TextView
                    android:id="@+id/find_flyer_send_rate_text"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:drawableLeft="@drawable/ic_local_offer_black_24px"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:gravity="center"
                    android:text="0%"
                    android:layout_margin="8dp"/>
                <TextView
                    android:id="@+id/find_flyer_read_rate_text"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:drawableLeft="@drawable/ic_remove_red_eye_black_24px"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:gravity="center"
                    android:text="0%"
                    android:layout_margin="8dp"/>
